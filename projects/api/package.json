{
  "name": "@frakas/api",
  "version": "0.0.11",
  "description": "Frakas multiplayer framework API",
  "files": [
    "*.js",
    "*.ts",
    "*.map",
    "**/*.js",
    "**/*.ts",
    "**/*.map"
  ],
  "scripts": {
    "build2": "npx webpack && npx tsc --emitDeclarationOnly --outDir publish",
    "build": "del tsconfig.tsbuildinfo -f && tsc --outDir publish",
    "debug": "node --inspect=9229 build/app.js",
    "publish": "cd publish && npm publish --access public",
    "prePublish": "node ../../scripts/prePublish.js",
    "all": "npm run build && npm run prePublish && npm run publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamhitori/frakas.git"
  },
  "keywords": [],
  "author": "ReubenH",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teamhitori/frakas/issues"
  },
  "homepage": "https://github.com/teamhitori/frakas#readme",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^9.2.0",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "fs": "0.0.1-security",
    "rxjs": "^7.5.4",
    "uuid": "^8.3.2",
    "ws": "^8.6.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/rx": "^4.1.2",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "ts-loader": "^9.3.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2"
  }
}